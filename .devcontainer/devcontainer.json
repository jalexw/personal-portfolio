{
  "name": "jalexw Next.js Portfolio Development Server",
  "image": "oven/bun:1.3.3",
  "postCreateCommand": "bun install",
  "postAttachCommand": {
    "server": "bun run dev"
  },
  "forwardPorts": [3000],
  "portsAttributes": {
    "3000": {
      "label": "jalexw Portfolio Dev Server",
      "onAutoForward": "openPreview"
    }
  },
  "customizations": {
    "codespaces": {
      "openFiles": ["README.md"]
    },
    "vscode": {
      "extensions": []
    }
  }
}
